{
    "shell": "src/main-application.js",
    "entrypoint": "index.html",
    "fragments": [
        "src/wc-header.js",
        "src/wc-body.js",
        "src/wc-body-textanalysis.js",
        "src/wc-footer.js"
    ],
    "npm": true,
    "moduleResolution": "node",
    "sources": [
        "src/main-application.js",
        "src/wc-header.js",
        "src/wc-body.js",
        "src/wc-body-textanalysis.js",
        "src/wc-footer.js",
        "src/main-styles.js",
        "lib/stopwords_en.js",
        "package.json"
    ],
    "extraDependencies": [
        "node_modules/@webcomponents/webcomponentsjs/**"
    ],
    "builds": [
        {
            "bundle": true,
            "js": {
                "minify": false,
                "compile": "es5",
                "transformModulesToAmd": true
            },
            "addServiceWorker": true,
            "addPushManifest": true
        }
    ]
}