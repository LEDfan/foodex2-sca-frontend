{
  "entrypoint": "index.html",
  "shell": "src\\main-application.js",
  "fragments": [
    "src/wc-header.js",
    "src/wc-body.js",
    "src/wc-body-analyse.js",
    "src/wc-body-classify.js",
    "src/wc-body-getcode.js",
    "src/wc-footer.js"
  ],
  "sources": [
    "src/**/*",
    "package.json",
    "index.html",
    "src/main-application.js",
    "src/wc-header.js",
    "src/wc-body.js",
    "src/wc-body-analyse.js",
    "src/wc-body-classify.js",
    "src/wc-body-getcode.js",
    "src/wc-footer.js"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "bundle": true,
      "js": {
        "minify": false,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "addServiceWorker": true,
      "addPushManifest": true
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ],
    "warningsToIgnore": []
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}